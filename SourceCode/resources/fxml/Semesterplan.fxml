<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<SplitPane dividerPositions="0.5" orientation="VERTICAL" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SemesterplanController">
    <items>
        <Pane prefHeight="96.0" prefWidth="1200.0">
            <children>
                <Button id="btnAdd" fx:id="addButton" layoutX="1067.0" layoutY="33.0" mnemonicParsing="false" onAction="#onAddBtnClicked" text="Hinzufügen" />
                <DatePicker id="sepDatePicker" fx:id="sepDatePicker" layoutX="37.0" layoutY="33.0" onAction="#chooseDate" promptText="Datum" showWeekNumbers="true" />
                <Button id="btnDelete" fx:id="deleteBtn" layoutX="983.0" layoutY="33.0" mnemonicParsing="false" onAction="#onDeleteBtnClicked" text="Löschen" textFill="RED" />
                <Text layoutX="460.0" layoutY="67.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Semesterplan">
                    <font>
                        <Font size="47.0" />
                    </font>
                </Text>
            </children>
        </Pane>
        <TableView fx:id="semesterplan" editable="true" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="487.0" prefWidth="1200.0" />
    </items>
</SplitPane>
