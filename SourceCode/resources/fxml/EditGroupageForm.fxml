        <Label fx:id="semesterLabel" layoutX="55.0" layoutY="165.0" text="Semester:" />
        <ComboBox fx:id="semesterComboBox" layoutX="180.0" layoutY="160.0" onAction="#addToSemester" prefWidth="150.0" AnchorPane.leftAnchor="180.0" AnchorPane.rightAnchor="196.0" />
      <Button fx:id="deleteStudent" layoutX="248.0" layoutY="347.0" mnemonicParsing="false" onAction="#onDeleteBTNClicked" text="Löschen" />
